{
  "tasks": {
    "dev": "deno run -A cli.ts -T $GITHUB_TOKEN -R 'kurtaking/octoherd-cli'"
  },
  "imports": {
    "/": "./",
    "./": "./",
    "@octoherd/octokit": "npm:@octoherd/octokit",
    "@octokit/auth-oauth-device": "npm:@octokit/auth-oauth-device",
    "@octokit/openapi-types": "npm:@octokit/openapi-types",
    "chalk": "npm:chalk",
    "clipboardy": "npm:clipboardy",
    "enquirer": "npm:enquirer",
    "jsonfile": "npm:jsonfile",
    "mkdirp": "npm:mkdirp",
    "tempy": "npm:tempy",
    "@octokit/openapi": "npm:@octokit/openapi",
    "yargs": "npm:yargs",
    "prettier": "npm:prettier"
  },
  "lint": {
    "include": ["scripts/", "bin/", "lib/"],
    "exclude": ["**/*test.ts"]
  }
}
